    "json-loader": "0.5.4",
    "just-wait": "^1.0.11",
    "npm-watch": "^0.1.8",
    "null-loader": "^0.1.1",
    "nyc": "^10.1.2",
    "object-assign": "4.1.0",
    "path": "^0.12.7",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "prop-types": "latest",
    "raw-loader": "^0.5.1",
    "react-dev-utils": "^0.4.2",
    "react-router-redux": "4.0.8",
    "recursive-readdir": "2.1.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-thunk": "2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^3.2.1",
    "script-loader": "^0.7.0",
    "strip-ansi": "3.0.1",
    "style-loader": "^0.13.1",
    "truffle-contract": "^1.1.8",
    "truffle-solidity-loader": "0.0.8",
    "uglify-loader": "^1.3.0",
    "url-loader": "0.5.7",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "auth0-lock": "10.14.0",
    "babel-cli": "6.24.1",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "dotenv": "^2.0.0",
    "express": "4.15.2",
    "moment": "2.18.1",
    "pug": "2.0.0-beta.12",
    "react": "15.4.0",
    "react-bootstrap-table": "3.1.7",
    "react-dom": "15.4.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-redux": "5.0.4",
    "react-router": "3.0.4",
    "redux": "3.6.0",
    "redux-form": "6.6.2",
    "redux-ui": "0.0.15",
    "source-map-support": "0.4.14"
  },
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development node tmp/server.js",
    "dev:server:wait": "just-wait -p \"tmp/server.js\" --timeout 120 && npm-watch",
    "dev:sourcemaps": "cross-env SOURCEMAP=true npm run dev",
    "dev": "rimraf tmp && concurrently --kill-others \"npm run hot:server\" \"npm run hot:client\" \"npm run dev:server:wait\"",
    "build": "node scripts/build.js",
    "test-jest": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage",
    "clean:dist": "rimraf dist",
    "clean:js": "rimraf public/js/main*.js && rimraf public/js/app.js && rimraf public/js/plugins.js && rimraf public/js/*.map",
    "clean:css": "rimraf public/css/main*.css && rimraf public/css/app*.css && rimraf public/css/*.map",
    "clean:fonts": "rimraf public/fonts/*",
    "clean:server": "rimraf server.js && rimraf server.js.map",
    "clean:tmp": "rimraf tmp",
    "clean": "run-s clean:dist clean:tmp clean:server clean:css clean:js clean:fonts",
    "hot:client": "cross-env NODE_ENV=development EXTRACT_TEXT_PLUGIN=false webpack-dev-server --config tools/webpack.client.babel.js",
    "hot:server": "cross-env NODE_ENV=development WEBPACK_DEV_SERVER=true webpack --colors --display-error-details --watch --verbose --config tools/webpack.server.babel.js",
    "start-watcher": "npm-watch",
    "watch-build-server": "run-s clean:tmp hot:server",
    "dev:dist": "run-p dev dist:watch",
    "build:server": "cross-env NO_OUTPUT_PATH=true NODE_ENV=production webpack --progress --colors --display-error-details --config tools/webpack.server.babel.js",
    "build:client": "cross-env EXTRACT_TEXT_PLUGIN=true webpack --progress --colors --display-error-details --config tools/webpack.client.babel.js",
    "bundle:server": "run-s clean:tmp clean:server build:server",
    "bundle:client": "run-s clean:css clean:js clean:fonts build:client",
    "prod:server": "cross-env NODE_ENV=production node tmp/server.js",
    "build:prod": "cross-env NODE_ENV=production run-s bundle:client bundle:server",
    "prod": "run-s build:prod prod:server",
    "dist": "cross-env NODE_ENV=production run-s bundle:client && node tools/create-dist.js",
    "dist:watch": "run-s bundle:client && node tools/watch-dist.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register ./test/setup.js ./test/**/*.spec.js",
    "test:watch": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --watch ./test/setup.js ./test/**/*.spec.js",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha ./test/setup.js ./test/**/*.spec.js",
    "test:coverage:jenkins": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha ./test/setup.js ./test/**/*.spec.js --reporter mocha-junit-reporter || exit 0",
    "start": "babel-node server.babel.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>\\config\\polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>\\config\\jest\\cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>\\config\\jest\\fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": false
  }
}
